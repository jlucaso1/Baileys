{
  "name": "baileys",
  "version": "6.7.8",
  "description": "WhatsApp API",
  "keywords": [
    "whatsapp",
    "js-whatsapp",
    "whatsapp-api",
    "whatsapp-web",
    "whatsapp-chat",
    "whatsapp-group",
    "automation",
    "multi-device"
  ],
  "homepage": "https://github.com/WhiskeySockets/Baileys",
  "repository": {
    "url": "git@github.com:WhiskeySockets/Baileys.git"
  },
  "type": "module",
  "license": "MIT",
  "author": "Adhiraj Singh",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "lib/*",
    "WAProto/*",
    "WASignalGroup/*.js"
  ],
  "scripts": {
    "build:all": "tsc && typedoc",
    "build:docs": "typedoc",
    "build:tsc": "tsc",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "tsx Example/example.ts",
    "example:mobile": "tsx Example/example.ts --mobile",
    "format": "biome format --write src/**/*.ts",
    "lint": "biome lint --write src/**/*.ts",
    "prepack": "tsc",
    "release": "release-it",
    "test": "bun test"
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^9.1.3",
    "@noble/ciphers": "^1.0.0",
    "@noble/hashes": "^1.5.0",
    "async-lock": "^1.4.1",
    "axios": "^1.6.0",
    "cache-manager": "^5.7.6",
    "libsignal": "npm:@jlucaso/libsignal-node@2.1.3",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "pbf": "^4.0.1",
    "pino": "^7.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/bun": "^1.1.8",
    "@types/long": "^5.0.0",
    "@types/sharp": "^0.29.4",
    "@types/ws": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "json": "^11.0.0",
    "libphonenumber-js": "^1.10.20",
    "open": "^8.4.2",
    "release-it": "^15.10.3",
    "tsx": "^4.19.0",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "audio-decode": "^2.1.3",
    "jimp": "^0.16.1",
    "link-preview-js": "^3.0.0",
    "qrcode-terminal": "^0.12.0",
    "music-metadata": "^7.12.3",
    "sharp": "^0.32.6"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    },
    "qrcode-terminal": {
      "optional": true
    },
    "sharp": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.19",
  "trustedDependencies": [
    "protobufjs"
  ]
}
